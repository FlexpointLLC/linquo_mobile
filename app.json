{
  "expo": {
    "name": "Linquo",
    "slug": "linquo-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "linquo",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.linquo.app"
    },
  "android": {
    "icon": "./assets/app-icon.png",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#4A90E2",
      "mdpi": "./assets/splash-icon.png",
      "hdpi": "./assets/splash-icon.png",
      "xhdpi": "./assets/splash-icon.png",
      "xxhdpi": "./assets/splash-icon.png",
      "xxxhdpi": "./assets/splash-icon.png"
    },
    "package": "com.linquo.app",
    "googleServicesFile": "./google-services.json",
    "softwareKeyboardLayoutMode": "resize",
    "edgeToEdgeEnabled": true,
    "predictiveBackGestureEnabled": false,
    "intentFilters": [
      {
        "action": "VIEW",
        "autoVerify": true,
        "data": [
          {
            "scheme": "linquo"
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ]
  },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@react-native-firebase/app"

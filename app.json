{
  "expo": {
    "name": "Linquo",
    "slug": "linquo-mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/app-icon.png",
    "userInterfaceStyle": "automatic",
    "scheme": "linquo",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.linquo.app"
    },
  "android": {
    "icon": "./assets/app-icon.png",
    "package": "com.linquo.app",
    "softwareKeyboardLayoutMode": "resize",
    "edgeToEdgeEnabled": true,
    "predictiveBackGestureEnabled": false,
    "intentFilters": [
      {
        "action": "VIEW",
        "autoVerify": true,
        "data": [
          {
            "scheme": "linquo"
          }
        ],
        "category": [
          "BROWSABLE",
          "DEFAULT"
        ]
      }
    ]
  },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [],
    "extra": {
      "eas": {
        "projectId": "9307bed5-21cb-400a-9213-ace7b985e270"
      }
    },
    "owner": "ashikprottoy"
  }
}
